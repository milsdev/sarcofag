{
    "name": "milsdev/wordpress-plugin-sarcofag",
    "description": "Wordpress plugin, brings EventDispatcher, DependencyInjection and MVC to the Wordpress.",
    "type": "wordpress-muplugin",
    "license": "GPL-2.0",
    "authors": [
        {
            "name": "Mil's",
            "email": "hi@milsdev.com"
        }
    ],
    "require": {
        "composer/installers": "~1.0",
        "php-di/php-di": "5.2.2",
        "slim/slim": "3.4.*",
        "slim/php-view": "2.0.*",
        "mobiledetect/mobiledetectlib": "~2.8",
        "php": "^5.5 || ^7.0"
    },
    "require-dev": {
      "phpunit/PHPUnit": "~4.0",
      "squizlabs/php_codesniffer": "^2.0",
      "satooshi/php-coveralls": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "Sarcofag\\": "src/"
        }
    },
    "autoload-dev": {
      "psr-4": {
        "SarcofagTest\\": "test/",
        "SarcofagBench\\": "benchmarks/"
      }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "bin": ["bin/sarcofag"],
    "extra": {
        "installer-name": "sarcofag"
    }
}
